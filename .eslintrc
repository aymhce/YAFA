{
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "mocha": true
  },
  "globals": {
    "fetch": true
  },
  "rules": {
    "arrow-body-style": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "no-return-assign": "off",
    "linebreak-style": "off",
    "semi": "off"
  }
}
